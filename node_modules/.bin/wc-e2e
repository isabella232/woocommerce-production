#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/github/workspace/packages/js/e2e-environment/bin/node_modules:/github/workspace/packages/js/e2e-environment/node_modules:/github/workspace/packages/js/node_modules:/github/workspace/packages/node_modules:/github/workspace/node_modules:/github/node_modules:/node_modules:/github/workspace/plugins/woocommerce/node_modules:/github/workspace/plugins/node_modules"
else
  export NODE_PATH="$NODE_PATH:/github/workspace/packages/js/e2e-environment/bin/node_modules:/github/workspace/packages/js/e2e-environment/node_modules:/github/workspace/packages/js/node_modules:/github/workspace/packages/node_modules:/github/workspace/node_modules:/github/node_modules:/node_modules:/github/workspace/plugins/woocommerce/node_modules:/github/workspace/plugins/node_modules"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../../../../packages/js/e2e-environment/bin/wc-e2e.sh" "$@"
else
  exec bash  "$basedir/../../../../packages/js/e2e-environment/bin/wc-e2e.sh" "$@"
fi
